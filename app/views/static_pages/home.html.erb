<% if logged_in? %>
  <script type="text/javascript">
    window.location.href="<%= user_path current_user %>"
  </script>    
<% else %>
  <div class="center jumbotron">
    <h1>Welcome to the Test App</h1><br/>
    <%= link_to "Sign up now!", signup_path , class: "btn btn-lg btn-primary" %>
  </div>
<% end %>